.col-4
    h4 To Do
    ul#todo-column.list-group(data-bind="sortable: {foreach: tasksTodo}")
        li.list-group-item(data-bind="text: titulo")
.col-4
    h4 Working ...
    ul#working-column.list-group(data-bind="sortable: {foreach: tasksWorking}")
        li.list-group-item(data-bind="text: titulo")
.col-4
    h4 Done
    ul#done-column.list-group(data-bind="sortable: {foreach: tasksDone}")
        li.list-group-item(data-bind="text: titulo")
